<Application x:Class="SDBS3000.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             xmlns:local="clr-namespace:SDBS3000"
             xmlns:o="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/SkinDefault.xaml" />
                <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/Theme.xaml" />

                <ResourceDictionary Source="pack://application:,,,/SDBS3000;component/Dictionary/Themes/DarkTheme.xaml" />
                <ResourceDictionary Source="Dictionary/FontFamily.xaml" />
                <ResourceDictionary Source="Dictionary/Converter.xaml" />
                <ResourceDictionary Source="Dictionary/Effects.xaml" />
                <ResourceDictionary Source="Dictionary/ImageBrushes.xaml" />

                <ResourceDictionary Source="Dictionary/Themes/Generic.xaml" />

                <ResourceDictionary Source="Dictionary/Styles/Button.xaml" />
                <ResourceDictionary Source="Dictionary/Styles/ToggleButton.xaml" />
                <ResourceDictionary Source="Dictionary/Styles/DataGrid.xaml" />
                <ResourceDictionary Source="Dictionary/Styles/RadioButton.xaml" />
                <ResourceDictionary Source="Dictionary/Styles/ComboBox.xaml" />
                <ResourceDictionary Source="Dictionary/Styles/TextBox.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
